{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "app",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/packages/app",
      "args": ["--debug"],
    },
    {
      "name": "go attach",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0
    },
    {
      "type": "node",
      "name": "gulp",
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
    },
    {
      "type": "node",
      "name": "gulp full",
      "args": [
        "full"
      ],
      "program": "${workspaceFolder}/node_modules/gulp/bin/gulp.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
    },
  ]
}
